{
  "name": "Month → Doc Switcher",
  "organization": "Snell",
  "description": "Dropdown selects a month and shows the embedded doc.",
  "logoUrl": "https://snellrojas.com/favicon.ico",
  "supportUrl": "https://snellrojas.com",
  "privacyPolicyUrl": "https://snellrojas.com/privacy",
